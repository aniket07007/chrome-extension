{
  "manifest_version": 3,

  "name": "Reminder",
  "description": "E-Sign software for car dealerships.",
  "version": "1.0",
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "1.jpg",
          "24": "1.jpg",
          "32": "1.jpg"
      }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "cookies",
    "scripting",
    "notifications",
    "storage"
  ],
  "host_permissions": [
    "http://*/*","https://*/*","file://*/*"
  ]
}
